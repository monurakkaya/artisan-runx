{
    "name": "monurakkaya/artisan-runx",
    "description": "Run multiple Artisan commands with a single command",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/process": "^10.0",
        "illuminate/container": "^10.0",
        "illuminate/contracts": "^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0",
        "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Monurakkaya\\ArtisanRunx\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Monurakkaya\\ArtisanRunx\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Mehmet Onur AKKAYA",
            "email": "monurakkaya@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Monurakkaya\\ArtisanRunx\\Providers\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
